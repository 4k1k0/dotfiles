alias touOff='xinput set-prop 12 "Device Enabled" 0'
alias touOn='xinput set-prop 12 "Device Enabled" 1'
alias actualizar='yaourt -Syua'
alias archivos='sudo pcmanfm'
alias instalar='sudo pacman -S'
alias remover='sudo pacman -Rs'
alias yinstalar='yaourt -S'
alias apagaren='sudo shutdown -h'
alias espacio='df --block-size=GB'
alias ..='cd ..'
alias imagenes='wget -nd -H -p -A jpg,jpeg,png,gif -e robots=off'
alias tOn='transmission-daemon && transmission-remote-cli'
alias tOff='killall transmission-daemon'
alias datos='cd /media/datos/ && ranger'
alias unrar='unrar x'
alias pantalla='sh .pantalla.sh'
