#Subir volumen
"amixer set Master 5%+"
	XF86AudioRaiseVolume
#Bajar volumen
"amixer set Master 5%-"
	XF86AudioLowerVolume
#Mute  volumen
"amixer set Master toggle"
	XF86AudioMute
#Play/Pause
"mpc toggle"
	XF86AudioPlay
# Next
"mpc next"
	Mod4 + KP_Right
# Back
"mpc prev"
	Mod4 + KP_Left 
# Back
###############################
#Captura de pantalla
"xfce4-screenshooter"
	Print
#Bloquear pantalla
"i3lock -i ~/.lock.png"
	XF86ScreenSaver
#Brillo de pantalla +
"xbacklight -inc 10"
	XF86MonBrightnessUp
#Brillo de pantalla -
"xbacklight -dec 10"
	XF86MonBrightnessDown
# Prender torrents
"transmission-daemon"
	Mod4 + KP_Down
# Apagar torrents
"killall transmission-daemon"
	Mod4 + KP_Up
